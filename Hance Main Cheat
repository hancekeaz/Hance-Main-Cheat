    local function toggleVisibility(visible)
        textBox.Visible = visible
        playButton.Visible = visible
        stopButton.Visible = visible
        volumeBox.Visible = visible
        pitchBox.Visible = visible
        timeBox.Visible = visible
        loopLabel.Visible = visible
        loopButton.Visible = visible
    end

    minimizeButton.MouseButton1Click:Connect(function()
        if isMinimized then
            toggleVisibility(true)
            local expandTween = TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 350, 0, 250),
                Position = lastPosition
            })
            expandTween:Play()
        else
            lastPosition = mainFrame.Position
            toggleVisibility(false)
            local minimizeTween = TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 350, 0, 35),
                Position = UDim2.new(lastPosition.X.Scale, lastPosition.X.Offset, lastPosition.Y.Scale, lastPosition.Y.Offset)
            })
            minimizeTween:Play()
        end
        isMinimized = not isMinimized
        playSound("rbxassetid://243152215")
    end)

    local function playAudio(musicId, volume, pitch, timePosition)
        if not randomName then
            randomName = generateRandomName()
        end

        local assetId = "rbxassetid://" .. musicId
        local success, err = pcall(function()
            local currentRemote = getRemoteEvent()
            if currentRemote then
                local args = {
                    [1] = "newSound",
                    [2] = randomName,
                    [3] = InjectTo,
                    [4] = assetId,
                    [5] = pitch,
                    [6] = volume,
                    [7] = loopEnabled,
                    [8] = timePosition
                }
                currentRemote:FireServer(unpack(args))
                currentRemote:FireServer("playSound", randomName)
                isPlaying = true
                currentSoundId = musicId
            else
                error("RemoteEvent not found!")
            end
        end)

        if not success then
            playSound("rbxassetid://9066167010")
            NotificationLibrary:SendNotification("Error", "ðŸ˜¢ Failed to connect to AC6 (Vehicle despawn?)", 5, "9066167010")
        else
            playSound("rbxassetid://156785206")
            NotificationLibrary:SendNotification("Success", "Playing audio ID: " .. musicId, 3, "156785206")
        end
    end

    local function stopAudio()
        if isPlaying and randomName then
            local success, err = pcall(function()
                local currentRemote = getRemoteEvent()
                if currentRemote then
                    currentRemote:FireServer("stopSound", randomName)
                    isPlaying = false
                    currentSoundId = nil
                else
                    error("RemoteEvent not found!")
                end
            end)
            if not success then
                playSound("rbxassetid://9066167010")
                NotificationLibrary:SendNotification("Error", "ðŸ˜¢ Failed to stop audio", 5, "9066167010")
            else
                playSound("rbxassetid://156785206")
                NotificationLibrary:SendNotification("Success", "Audio stopped", 3, "156785206")
            end
        end
    end

    playButton.MouseButton1Click:Connect(function()
        local musicId = textBox.Text
        local volume = math.clamp(tonumber(volumeBox.Text) or 1, 0, 10)
        local pitch = math.clamp(tonumber(pitchBox.Text) or 1, 0.5, 2)
        local timePosition = tonumber(timeBox.Text) or 0
        if musicId and musicId ~= "" then
            playAudio(musicId, volume, pitch, timePosition)
        else
            NotificationLibrary:SendNotification("Error", "Please enter a valid Music ID", 3, "9066167010")
        end
    end)

    stopButton.MouseButton1Click:Connect(stopAudio)

    -- // Keyboard shortcuts
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed then
            if input.KeyCode == Enum.KeyCode.P then
                playButton:Activate()
            elseif input.KeyCode == Enum.KeyCode.S then
                stopButton:Activate()
            elseif input.KeyCode == Enum.KeyCode.M then
                minimizeButton:Activate()
            end
        end
    end)

else
    playSound("rbxassetid://9066167010")
    NotificationLibrary:SendNotification("Error", "ðŸ’” AC6 not found in this game!", 5, "9066167010")
end
   end,
})

