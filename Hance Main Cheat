   local player = game.Players.LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")
local workspace = game:GetService("Workspace")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

-- // Parent audio to TestService to avoid anti-cheat deletion
local InjectTo = game:GetService("TestService")

local specialChars = "1234567890QWRYUIOPASDFGHJKLZXVBMN+Ã—Ã·=/_<>,!@#$%^&*?`~\\|{}â‚¬Â£Â¥â‚©qwertyuiopasdfghjklzxcvbnm"
local randomName = nil
local currentSoundId = nil
local isPlaying = false

local function generateRandomName()
    local length = math.random(8, 12)
    local name = ""
    for i = 1, length do
        local index = math.random(1, #specialChars)
        name = name .. specialChars:sub(index, index)
    end
    return name
end

local function findRemoteEvent(name, parent)
    for _, child in ipairs(parent:GetDescendants()) do
        if child:IsA("RemoteEvent") and child.Name == name then
            return child
        end
    end
    return nil
end

local function getRemoteEvent()
    return findRemoteEvent("AC6_FE_Sounds", replicatedStorage) or findRemoteEvent("AC6_FE_Sounds", workspace)
end

local function playSound(soundId)
    local sound = Instance.new("Sound")
    sound.SoundId = soundId
    sound.Volume = 1
    sound.PlayOnRemove = true
    sound.Parent = workspace
    sound:Destroy()
end

-- // Advanced Notification library by IceMinister#9889
local NotificationLibrary = {}
local CoreGui = cloneref(game:GetService("CoreGui"))
local library
local templateFolder
local canvas

function NotificationLibrary:Load()
    library = game:GetObjects("rbxassetid://15133757123")[1]
    templateFolder = library.Templates
    canvas = library.list
    library.Name = "Notification Library"
    library.Parent = CoreGui
end

function NotificationLibrary:SendNotification(Mode, Text, Duration, IconId)
    local libaryCore = CoreGui:FindFirstChild("Notification Library")
    if not libaryCore then
        NotificationLibrary:Load()
    else
        library = libaryCore
        templateFolder = library.Templates
        canvas = library.list
    end
    if templateFolder:FindFirstChild(Mode) then
        task.spawn(function()
            local success, err = pcall(function()
                local Notification = templateFolder:WaitForChild(Mode):Clone()
                local filler = Notification.Filler
                local bar = Notification.bar
                local icon = Notification.Icon
                Notification.Header.Text = Text
                if IconId then
                    icon.Image = "rbxassetid://" .. IconId
                end
                
                Notification.Visible = true
                Notification.Parent = canvas
    
                Notification.Size = UDim2.new(0, 0, 0.1, 0)
                filler.Size = UDim2.new(1, 0, 1, 0)
        
                local T1 = TweenInfo.new(0.3, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
                local T2 = TweenInfo.new(Duration, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
                local T3 = TweenInfo.new(0.4, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
            
                TweenService:Create(Notification, T1, {Size = UDim2.new(1, 0, 0.1, 0)}):Play()
                task.wait(0.3)
                TweenService:Create(filler, T3, {Size = UDim2.new(0.01, 0, 1, 0)}):Play()
            
                TweenService:Create(bar, T2, {Size = UDim2.new(1, 0, 0.06, 0)}):Play()
            
                task.wait(Duration)
            
                TweenService:Create(filler, T1, {Size = UDim2.new(1, 0, 1, 0)}):Play()
                task.wait(0.3)
                TweenService:Create(Notification, T3, {Size = UDim2.new(0, 0, 0.1, 0)}):Play()
                task.wait(0.4)
                Notification:Destroy()
            end)
            if not success then
                warn("Error creating notification: ", err)
            end
        end)
    else
        warn("Invalid notification theme")
    end
end

